{
  "name": "@mythos/sync",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./native": {
      "types": "./src/native/sqliteDb.ts",
      "import": "./src/native/sqliteDb.ts",
      "require": "./src/native/sqliteDb.ts"
    },
    "./web": {
      "types": "./src/web/dexieDb.ts",
      "import": "./src/web/dexieDb.ts",
      "require": "./src/web/dexieDb.ts"
    }
  },
  "dependencies": {
    "@mythos/state": "workspace:*",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@mythos/typescript-config": "workspace:*",
    "@types/uuid": "^10.0.0",
    "dexie": "^4.0.10",
    "typescript": "~5.7.2"
  },
  "peerDependencies": {
    "expo-sqlite": "~15.0.0",
    "dexie": "^4.0.10"
  },
  "peerDependenciesMeta": {
    "expo-sqlite": {
      "optional": true
    },
    "dexie": {
      "optional": true
    }
  }
}
