{
  "name": "@mythos/manifest",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./src/types.ts"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "default": "./src/hooks/index.ts"
    },
    "./builders": {
      "types": "./src/builders/index.ts",
      "default": "./src/builders/index.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@mythos/core": "workspace:*"
  },
  "devDependencies": {
    "@mythos/typescript-config": "workspace:*",
    "@types/react": "~19.1.10",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  }
}
