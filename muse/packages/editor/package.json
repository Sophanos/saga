{
  "name": "@mythos/editor",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./extensions/*": {
      "types": "./src/extensions/*.ts",
      "default": "./src/extensions/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@tiptap/core": "^2.10.4",
    "@tiptap/extension-document": "^2.10.4",
    "@tiptap/extension-heading": "^2.10.4",
    "@tiptap/extension-history": "^2.10.4",
    "@tiptap/extension-paragraph": "^2.10.4",
    "@tiptap/extension-placeholder": "^2.10.4",
    "@tiptap/extension-text": "^2.10.4",
    "@tiptap/pm": "^2.10.4",
    "@tiptap/react": "^2.10.4",
    "@tiptap/starter-kit": "^2.10.4",
    "@tiptap/suggestion": "^2.10.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@mythos/core": "workspace:*",
    "@mythos/typescript-config": "workspace:*",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "typescript": "^5.7.2"
  }
}
