{
  "name": "@mythos/ai",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./agents/*": {
      "types": "./src/agents/*.ts",
      "default": "./src/agents/*.ts"
    },
    "./prompts/*": {
      "types": "./src/prompts/*.ts",
      "default": "./src/prompts/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "@ai-sdk/provider": "^3.0.0",
    "@mythos/prompts": "workspace:*",
    "ai": "^6.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@mythos/core": "workspace:*",
    "@mythos/typescript-config": "workspace:*",
    "typescript": "^5.7.2"
  }
}
