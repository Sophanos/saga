{
  "expo": {
    "name": "Mythos IDE",
    "slug": "mythos-ide",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "rhei",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mythos.ide"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-system-ui",
        {
          "backgroundColor": "#09090b"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
